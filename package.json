{
  "name": "tcmb-xml-rates",
  "version": "1.0.0",
  "description": "TCMB Kur Paketi - Modern, Type-safe, Promise-based. Fetches exchange rates from Central Bank of the Republic of Turkey.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/tcmb-xml-rates.git"
  },
  "keywords": [
    "tcmb",
    "kurlar",
    "exchange-rates",
    "central-bank",
    "turkey",
    "try",
    "currency",
    "döviz",
    "merkez-bankası",
    "xml"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fast-xml-parser": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  }
}
